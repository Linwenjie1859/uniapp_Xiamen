<view class="content"><view class="header"><view data-event-opts="{{[['tap',[['search',['$event']]]]]}}" class="input_view round" bindtap="__e"><text class="text-lg text-grey">搜索商品</text></view><view class="after" style="{{'opacity:'+(afterHeaderOpacity)+';'+('z-index:'+(afterHeaderzIndex)+';')+('height:'+(StatusAddNav+'px')+';')}}"></view></view><view class="banner"><view class="uni-padding-wrap"><view class="page-section swiper"><view class="page-section-spacing"><swiper class="swiper" indicator-color="#fff" indicator-active-color="#51c77d" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}"><block wx:for="{{allInfo.banner}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><image src="{{item.pic}}" mode></image></swiper-item></block></swiper></view></view></view></view><view class="notice"><text class="cuIcon-notificationfill text-orange margin-left margin-right-xs"></text><swiper class="notice_view" vertical="true" autoplay="true" circular="true" interval="3000"><block wx:for="{{allInfo.roll}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><navigator class="font-28 text_limit" url="{{item.url}}">{{item.info}}</navigator></swiper-item></block></swiper></view><view class="home_list"><view class="list_top font-28"><block wx:for="{{allInfo.menus}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['fengyangScenery',['$event']]]]]}}" class="top_view" bindtap="__e"><image src="{{item.pic}}" mode></image><text>{{item.name}}</text></view></block></view><image class="radius home_pic2" src="{{allInfo.banner[1].pic}}" mode></image><view class="list_down"><image src="{{allInfo.article_menus[0].pic}}" mode data-event-opts="{{[['tap',[['fengyang_scenery',['$event']]]]]}}" bindtap="__e"></image><image src="{{allInfo.article_menus[1].pic}}" mode data-event-opts="{{[['tap',[['flowering_seasons',['$event']]]]]}}" bindtap="__e"></image><image src="{{allInfo.article_menus[2].pic}}" mode data-event-opts="{{[['tap',[['fruit_garden',['$event']]]]]}}" bindtap="__e"></image></view></view><view class="home_list"><view class="title"><text class="font-32">玩转凤阳</text><text data-event-opts="{{[['tap',[['idealLife',['$event']]]]]}}" class="font-28 gray" bindtap="__e">更多></text></view><image class="home_pic3" src="{{allInfo.info.otherList[0].image}}" mode data-id="{{allInfo.info.otherList[0].id}}" data-event-opts="{{[['tap',[['line_details',['$event']]]]]}}" bindtap="__e"></image><view class="recommend_list font-28"><block wx:for="{{allInfo.info.otherList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="list_view" data-id="{{item.id}}" data-event-opts="{{[['tap',[['line_details',['$event']]]]]}}" bindtap="__e"><image src="{{item.image}}" mode></image><text>{{item.store_name}}</text></view></block></view></view><view class="home_list"><view class="title"><text class="font-32">放心农产品</text><text data-event-opts="{{[['tap',[['agriculturalSpecialty',['$event']]]]]}}" class="font-28 gray" bindtap="__e">更多></text></view><view class="product_list font-28"><block wx:for="{{allInfo.info.bastList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="prod_view" data-id="{{item.id}}" data-event-opts="{{[['tap',[['goodeDetails',['$event']]]]]}}" bindtap="__e"><image src="{{item.image}}" mode></image><text class="font-28 text_limit" style="width:335rpx;display:block;text-align:left;">{{item.store_name}}</text><view class="list_info"><text class="font-24 orange">{{"￥"+item.ot_price}}</text><text class="font-24">{{"月售"+item.sales+"件"}}</text></view></view></block></view></view><text class="no_more font-28 gray">————没有更多————</text></view>